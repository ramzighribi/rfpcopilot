{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/_next/*", "/api/*", "/*.{css,scss,sass,js,json,ico,png,jpg,jpeg,gif,svg,webp,woff,woff2,ttf,eot,otf}"]
  },
  "routes": [
    {
      "route": "/_next/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    }
  ],
  "platform": {
    "apiRuntime": "node:20"
  },
  "trailingSlash": "auto"
}
